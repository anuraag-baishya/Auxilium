                    <a href="market"><img class="img-circle img-responsive img-center service-img"src="images/market.jpg" alt=""></a>
                    </figure>
                    <h3>Market</h3>
                </div>
                <div class="col-lg-4 col-sm-6 col-centered service wow animated slideInRight">
                    <a href="market"><img class="img-circle img-responsive img-center service-img"src="images/bnb.jpg" alt=""></a>
                    <h3>B & B</h3>
                </div>
            </div>
            <!-- <div class="row row-centered">
                <div class="col-lg-4 col-sm-6 col-centered service wow animated slideInLeft">
                    <a href="market"><img class="img-circle img-responsive img-center service-img"src="images/campus.jpg" alt=""></a>
                    <h3>Campus</h3>
                </div>
            </div> -->
            <div class="row row-centered">
                <div class="col-lg-4 col-sm-6 col-centered service wow animated slideInLeft">
                    <a href="market"><img class="img-circle img-responsive img-center service-img"src="images/places.jpg" alt=""></a>
                    <h3>Places</h3>
                </div>
                <div class="col-lg-4 col-sm-6 col-centered service wow animated slideInRight">
                    <a href="market"><img class="img-circle img-responsive img-center service-img"src="images/coupon.jpg" alt=""></a>
                    <h3>Promos</h3>
                </div>
            </div>
        </div>
    </section>

